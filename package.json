{
  "name": "gpt-pair-programmer",
  "version": "1.0.0",
  "description": "Pair Program with OpenAI locally or with ChatGPT.",
  "type": "module",
  "scripts": {
    "app": "nodemon -w package.json -w tsconfig.json -w tsconfig.app.json -w webpack.config.ts --exec \"yarn app:start\"",
    "app:start": "node --no-warnings=ExperimentalWarning --loader ts-node/esm ./node_modules/.bin/webpack serve",
    "server": "nodemon -w package.json -w tsconfig.json -w server --ext \"ts,yaml\" --ignore \"*.generated.*\" --exec \"yarn server:start\"",
    "server:start": "node --no-warnings=ExperimentalWarning --loader ts-node/esm server/index.ts",
    "server:build": "tsc --project tsconfig.json"
  },
  "keywords": [],
  "author": "me@levithomason.com",
  "license": "MIT",
  "engines": {
    "npm": "Use yarn only.",
    "yarn": ">=1.22.0"
  },
  "packageManager": "yarn@3.6.3",
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/debug": "^4.1.9",
    "@types/eslint": "^8.44.3",
    "@types/express": "^4.17.18",
    "@types/express-session": "^1.17.8",
    "@types/html-to-text": "^9.0.2",
    "@types/js-yaml": "^4.0.6",
    "@types/morgan": "^1.9.6",
    "@types/node": "^20.8.0",
    "@types/puppeteer": "^7.0.4",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "@types/validator": "^13.11.2",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.50.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "js-yaml": "^4.1.0",
    "prettier": "^3.0.3",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "esm-loader": "^0.1.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "glob": "^10.3.10",
    "highlight.js": "^11.8.0",
    "html-to-text": "^9.0.5",
    "langchain": "^0.0.157",
    "mermaid": "^10.5.0",
    "micromark": "^4.0.0",
    "micromark-extension-gfm": "^3.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1",
    "openai": "^4.11.0",
    "openapi-types": "^12.1.3",
    "puppeteer": "^21.3.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-markdown": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "remark-gfm": "^4.0.0",
    "sequelize": "^6.33.0",
    "sequelize-typescript": "^2.1.5",
    "sqlite3": "^5.1.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
