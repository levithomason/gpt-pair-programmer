{
  "name": "gpt-pair-programmer",
  "version": "1.0.0",
  "description": "Pair-program locally on your computer with local LLMs or as a ChatGPT plugin.",
  "type": "module",
  "scripts": {
    "app": "nodemon -w package.json -w tsconfig.json -w tsconfig.app.json -w webpack.config.ts -w shared --exec \"yarn app:start\"",
    "app:start": "DEBUG=\"gpp:*\" node --no-warnings=ExperimentalWarning --loader ts-node/esm ./node_modules/.bin/webpack serve",
    "server": "nodemon -w package.json -w tsconfig.json -w server -w shared --ext \"ts,yaml\" --ignore \"*.generated.*\" --exec \"yarn server:start\"",
    "server:start": "DEBUG=\"gpp:*\" node --no-warnings=ExperimentalWarning --loader ts-node/esm server/index.ts",
    "server:build": "tsc --project tsconfig.json",
    "fix-deps": "node scripts/fix-deps.js"
  },
  "keywords": [],
  "author": "me@levithomason.com",
  "license": "MIT",
  "engines": {
    "npm": "Use yarn only.",
    "yarn": ">=1.22.0",
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@3.6.3",
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/debug": "^4.1.9",
    "@types/eslint": "^8.44.3",
    "@types/express": "^4.17.18",
    "@types/html-to-text": "^9.0.2",
    "@types/js-yaml": "^4.0.6",
    "@types/morgan": "^1.9.6",
    "@types/node": "^20.8.2",
    "@types/puppeteer": "^7.0.4",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.10",
    "@types/validator": "^13.11.2",
    "@types/ws": "^8.5.6",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "css-loader": "^6.8.1",
    "eslint": "^8.50.0",
    "esm-loader": "^0.1.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "js-yaml": "^4.1.0",
    "prettier": "^3.0.3",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
