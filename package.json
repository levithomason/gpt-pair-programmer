    "express": "^4.18.2",
    "globby": "^13.2.2",
    "highlight.js": "^11.8.0",
    "html-to-text": "^9.0.5",
    "js-tiktoken": "^1.0.7",
    "mermaid": "^10.5.0",
    "micromark": "^4.0.0",
    "micromark-extension-gfm": "^3.0.0",
    "morgan": "^1.10.0",
    "neo4j-driver": "^5.14.0",
    "nodemon": "^3.0.1",
